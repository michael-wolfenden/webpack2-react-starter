{
  "name": "webpack2-react-starter",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run validate",
    "start": "webpack-dev-server --config config/webpack.config.development.js -d --hot --progress --open",
    "production": "webpack --config config/webpack.config.production.js -p --bail --optimize-dedupe",
    "test": "karma start config/karma.conf.js",
    "validate": "npm-run-all --parallel _lint _test:single-run",
    "_precommit-message": "echo '========= Executing pre-commit checks =========' && exit 0",
    "_lint": "eslint . --ignore-path .gitignore",
    "_test:single-run": "npm run test -- --single-run"
  },
  "pre-commit": [
    "_precommit-message",
    "validate"
  ],
  "keywords": [],
  "author": "Michael Wolfenden",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-1": "6.5.0",
    "clean-webpack-plugin": "0.1.9",
    "copy-webpack-plugin": "3.0.1",
    "enzyme": "2.3.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-import-resolver-webpack": "0.3.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "1.9.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "expect": "1.20.1",
    "html-webpack-plugin": "2.21.0",
    "karma": "1.1.0",
    "karma-mocha": "1.1.0",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "npm-run-all": "2.2.2",
    "phantomjs-prebuilt": "2.1.7",
    "pre-commit": "1.1.3",
    "react-addons-test-utils": "15.1.0",
    "webpack": "2.1.0-beta.13",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-md5-hash": "0.0.5"
  },
